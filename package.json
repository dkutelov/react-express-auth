{
	"name": "emaily-server-side",
	"version": "1.0.0",
	"description": "Express/ Node server side of Emaily app.",
	"main": "index.js",
	"engines": {
		"node": "8.9.4",
		"npm": "5.6.0"
	},
	"scripts": {
		"start": "node index.js",
		"server": "nodemon index.js",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
	"proxy": {
		"/surveys": {
			"target": "http://localhost:3000"
		}
	},
	"author": "dk",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.2",
		"concurrently": "^3.5.1",
		"connect-flash": "^0.1.1",
		"cookie-session": "^2.0.0-beta.3",
		"express": "^4.16.2",
		"mongoose": "^5.0.5",
		"nodemon": "^1.14.12",
		"passport": "^0.4.0",
		"passport-facebook": "^2.1.1",
		"passport-google-oauth20": "^1.0.0",
		"passport-http": "^0.3.0",
		"passport-local": "^1.0.0"
	}
}
